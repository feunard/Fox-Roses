{
  "name": "fox",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "productName": "Fox&Roses",
  "main": "build/electron.js",
  "author": "Feunard",
  "description": "Fox&Roses ~ by Feunard",
  "devDependencies": {
    "electron": "^15.3.0",
    "@types/node": "^16.11.5",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "cra-template-typescript": "1.1.2",
    "electron-builder": "^22.13.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron": "electron build/electron.js",
    "dist": "electron-builder build",
    "release": "npm run build && npm run dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "io.github.feunard.fox",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": [
        "portable",
        "dir"
      ],
      "icon": "build/icon.png"
    },
    "portable": {
      "unicode": false,
      "artifactName": "Fox&Roses.exe"
    }
  }
}
